{
    "name": "becklyn/rector",
    "description": "A collection of various refactoring rules for projects that use Rector and mainly Symfony",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Kai Eichinger",
            "email": "ke@becklyn.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "phpstan/phpstan": "^1.7.3",
        "rector/rector": ">=0.12.23"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/framework-bundle": "^5.4 || ^6.0"
    },
    "autoload": {
        "psr-4": {
            "Becklyn\\Rector\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Becklyn\\Rector\\": "tests/"
        }
    }
}
